{
  "autoprefixer": {
    "browsers": ["last 2 versions", "ie >=9", "iOS >=6", "Android >= 2.3"]
  },
  "directories": {
    "src": "src/",
    "dist": "dist/"
  },
  "ftp": {
    "host": "www.veeck.de",
    "user": "www.veeck.de",
    "remotePath": "."
  },
  "imagemin": {
    "progressive": true
  },
  "htmlmin": {
    "collapseWhitespace": true
  },
  "ports": {
    "livereload": 35729,
    "express": 4000
  }
}