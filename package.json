{
  "name": "homepage",
  "version": "6.0.0",
  "private": true,
  "homepage": "https://github.com/rejas/",
  "author": {
    "name": "rejas",
    "url": "https://github.com/rejas/"
  },
  "browserslist": [
    "last 3 versions",
    "not IE <= 11",
    "not IE_Mob <= 11",
    "not dead"
  ],
  "pre-push": [
    "test"
  ],
  "scripts": {
    "--- MAIN SCRIPTS ---": "------------------------------------------------------------",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "--- GATSBY SCRIPTS ---": "----------------------------------------------------------",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write *.json content/**/*.mdx src/**/*.{js,jsx}",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "--- NPM SCRIPTS ---": "-------------------------------------------------------------",
    "update": "npm-check -u",
    "--- CUSTOM SCRIPTS (TODO) ---": "---------------------------------------------------",
    "release": "bump package.json package-lock.json CHANGELOG.md --tag && npm run build",
    "version": "auto-changelog --commit-limit false --package && git add CHANGELOG.md",
    "upload": "node scripts/upload.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@mdx-js/mdx": "^1.5.7",
    "@mdx-js/react": "^1.5.7",
    "fslightbox-react": "^1.4.9",
    "gatsby": "^2.19.28",
    "gatsby-image": "^2.2.42",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-mdx": "^1.0.75",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-remark-copy-linked-files": "^2.1.37",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-theme-material-ui": "^1.0.8",
    "gatsby-transformer-sharp": "^2.3.16",
    "gatsby-transformer-yaml": "^2.2.25",
    "github-webcomponents": "^1.3.0",
    "lodash": "^4.17.15",
    "material-ui-audio-player": "^1.0.7",
    "prop-types": "^15.7.2",
    "query-string": "^6.11.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-syntax-highlighter": "^12.2.1"
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "npm-check": "^5.9.2",
    "prettier": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md,mdx,yaml}": "prettier --write"
  }
}
