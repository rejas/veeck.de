{
  "name": "homepage",
  "version": "5.6.4",
  "private": true,
  "homepage": "https://github.com/rejas/homepage",
  "author": {
    "name": "rejas",
    "url": "https://github.com/rejas/"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11",
    "safari >= 7",
    "iOS >= 9",
    "Android >= 4.4"
  ],
  "pre-push": [
    "test"
  ],
  "scripts": {
    "--- MAIN SCRIPTS ---": "------------------------------------------------------------",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "--- GATSBY SCRIPTS ---": "----------------------------------------------------------",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,mdx}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "--- CUSTOM SCRIPTS (TODO) ---": "---------------------------------------------------",
    "release": "bump package.json package-lock.json CHANGELOG.md --tag && npm run build",
    "update": "npm-check -u",
    "version": "auto-changelog --commit-limit false --package && git add CHANGELOG.md",
    "upload": "node scripts/upload.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.7.2",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "gatsby": "^2.18.7",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-mdx": "^1.0.59",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-webfonts": "^1.1.0",
    "gatsby-remark-images": "^3.1.35",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-sharp": "^2.3.7",
    "github-webcomponents": "^1.3.0",
    "npm-check-updates": "^3.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  }
}
