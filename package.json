{
  "name": "homepage",
  "version": "6.0.0",
  "private": true,
  "homepage": "https://github.com/rejas/",
  "author": {
    "name": "rejas",
    "url": "https://github.com/rejas/"
  },
  "browserslist": [
    "last 3 versions",
    "not IE <= 11",
    "not IE_Mob <= 11",
    "not dead"
  ],
  "pre-push": [
    "test"
  ],
  "scripts": {
    "--- MAIN SCRIPTS ---": "------------------------------------------------------------",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "--- GATSBY SCRIPTS ---": "----------------------------------------------------------",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write *.json content/**/*.mdx src/**/*.{js,jsx}",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "--- NPM SCRIPTS ---": "-------------------------------------------------------------",
    "update": "npm-check -u",
    "--- CUSTOM SCRIPTS (TODO) ---": "---------------------------------------------------",
    "release": "bump package.json package-lock.json CHANGELOG.md --tag && npm run build",
    "version": "auto-changelog --commit-limit false --package && git add CHANGELOG.md",
    "testLive": "npm run build && npx http-server public"
  },
  "dependencies": {
    "@bigheads/core": "^0.3.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "fslightbox-react": "^1.6.2",
    "gatsby": "^2.32.4",
    "gatsby-background-image": "^1.4.1",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-mdx": "^1.10.0",
    "gatsby-plugin-no-sourcemaps": "^2.9.0",
    "gatsby-plugin-offline": "^3.10.1",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-remark-copy-linked-files": "^2.10.0",
    "gatsby-remark-images": "^3.11.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-theme-material-ui": "^1.0.13",
    "gatsby-transformer-sharp": "^2.12.0",
    "gatsby-transformer-yaml": "^2.11.0",
    "github-webcomponents": "^1.3.0",
    "lodash": "^4.17.20",
    "material-ui-audio-player": "^1.5.3",
    "prop-types": "^15.7.2",
    "query-string": "^6.14.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-spring": "^8.0.27",
    "react-syntax-highlighter": "^15.4.3",
    "remark-unwrap-images": "^2.0.0",
    "use-sound": "^2.0.1"
  },
  "devDependencies": {
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "npm-check": "^5.9.2",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md,mdx,yaml}": "prettier --write"
  }
}
