{
  "name": "homepage",
  "version": "5.6.4",
  "private": true,
  "homepage": "https://github.com/rejas/homepage",
  "author": {
    "name": "rejas",
    "url": "https://github.com/rejas/"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11",
    "safari >= 7",
    "iOS >= 9",
    "Android >= 4.4"
  ],
  "pre-push": [
    "test"
  ],
  "scripts": {
    "--- MAIN SCRIPTS ---": "------------------------------------------------------------",
    "start": "gulp dev",
    "build": "gulp",
    "test": "npm run test:gulp && npm run test:mocha",
    "--- CUSTOM SCRIPTS ---": "----------------------------------------------------------",
    "release": "bump package.json package-lock.json CHANGELOG.md --tag && npm run build",
    "test:gulp": "gulp check",
    "test:mocha": "mocha --recursive test",
    "update": "npm-check -u",
    "--- NPM SCRIPTS ---": "-------------------------------------------------------------",
    "version": "auto-changelog --commit-limit false --package && git add CHANGELOG.md",
    "--- TODO SCRIPTS ---": "------------------------------------------------------------",
    "analyze": "react-scripts build --stats && webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
    "cleanup": "prettier --write src/**/*.{jsx,js}",
    "upload": "node scripts/upload.js"
  },
  "dependencies": {
    "browser-update": "3.3.1",
    "core-js": "^3.3.5",
    "feather-icons": "^4.24.1",
    "github-webcomponents": "^1.3.0",
    "halkabox": "^1.6.0",
    "konami": "1.6.2",
    "lazy-observer-load": "^1.0.1",
    "normalize-opentype.css": "0.2.4",
    "object-fit-images": "3.2.4",
    "tinycolor2": "1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "assemble": "^0.24.3",
    "assemble-middleware-page-variable": "^0.1.0",
    "auto-changelog": "^1.16.2",
    "autoprefixer": "^9.7.0",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "chai": "^4.2.0",
    "css-loader": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "eslint": "^6.6.0",
    "eslint-friendly-formatter": "^4.0.1",
    "fancy-log": "^1.3.3",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-cache-bust": "^1.4.1",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-extname": "^0.2.2",
    "gulp-favicons": "^2.3.1",
    "gulp-flatten": "^0.4.0",
    "gulp-htmllint": "^0.0.16",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^6.1.1",
    "gulp-inject": "^5.0.4",
    "gulp-load-plugins": "^2.0.1",
    "gulp-louis": "^2.4.2",
    "gulp-modernizr": "^3.4.0",
    "gulp-prompt": "^1.2.0",
    "gulp-responsive": "^3.0.1",
    "gulp-sitemap": "^7.6.0",
    "gulp-stylelint": "^10.0.0",
    "gulp-uglify": "^3.0.2",
    "helper-md": "^0.2.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webp": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.0",
    "npm-check": "^5.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-sprites": "^4.2.1",
    "puppeteer": "^2.0.0",
    "sass-loader": "^8.0.0",
