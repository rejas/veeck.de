    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.12.0",
    "terser-webpack-plugin": "^2.2.1",
    "url-loader": "^2.2.0",
    "version-bump-prompt": "^5.0.5",
    "vinyl-ftp": "^0.6.1",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-merge": "^4.2.2",
    "webpack-stream": "^5.2.1"
  }
}
