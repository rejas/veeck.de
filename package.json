{
  "name": "homepage",
  "version": "6.0.0",
  "private": true,
  "homepage": "https://github.com/rejas/",
  "author": {
    "name": "rejas",
    "url": "https://github.com/rejas/"
  },
  "browserslist": [
    "last 3 versions",
    "not IE <= 11",
    "not IE_Mob <= 11",
    "not dead"
  ],
  "pre-push": [
    "test"
  ],
  "scripts": {
    "--- MAIN SCRIPTS ---": "------------------------------------------------------------",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "--- GATSBY SCRIPTS ---": "----------------------------------------------------------",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "--- NPM SCRIPTS ---": "-------------------------------------------------------------",
    "format": "prettier --write --ignore-unknown *.{js,json} content/**/*.mdx src/**/*.{js,jsx,css}",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "testLive": "npm run build && npx http-server public",
    "update": "npm-check -u"
  },
  "dependencies": {
    "@bigheads/core": "^0.3.1",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "fslightbox-react": "^1.6.2",
    "gatsby": "^3.7.1",
    "gatsby-background-image": "^1.5.3",
    "gatsby-plugin-feed": "^3.7.1",
    "gatsby-plugin-image": "^1.7.1",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-mdx": "^2.7.1",
    "gatsby-plugin-no-sourcemaps": "^3.7.0",
    "gatsby-plugin-offline": "^4.7.1",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-robots-txt": "^1.6.2",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-sitemap": "^4.3.1",
    "gatsby-remark-copy-linked-files": "^4.4.1",
    "gatsby-remark-images": "^5.4.1",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-theme-material-ui": "^2.0.1",
    "gatsby-transformer-sharp": "^3.7.1",
    "gatsby-transformer-yaml": "^3.7.1",
    "gbimage-bridge": "^0.1.4",
    "github-webcomponents": "^1.3.0",
    "lodash": "^4.17.21",
    "material-ui-audio-player": "^1.6.2",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-spring": "^9.2.3",
    "react-syntax-highlighter": "^15.4.3",
    "remark-unwrap-images": "^2.1.0",
    "use-sound": "^4.0.0"
  },
  "devDependencies": {
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "npm-check": "^5.9.2",
    "prettier": "^2.3.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
