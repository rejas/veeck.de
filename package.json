{
  "name": "homepage",
  "version": "5.6.4",
  "private": true,
  "homepage": "https://github.com/rejas/homepage",
  "author": {
    "name": "rejas",
    "url": "https://github.com/rejas/"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11",
    "safari >= 7",
    "iOS >= 9",
    "Android >= 4.4"
  ],
  "pre-push": [
    "test"
  ],
  "scripts": {
    "--- MAIN SCRIPTS ---": "------------------------------------------------------------",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "--- GATSBY SCRIPTS ---": "----------------------------------------------------------",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,mdx,yaml}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "--- NPM SCRIPTS ---": "-------------------------------------------------------------",
    "update": "npm-check -u",
    "--- CUSTOM SCRIPTS (TODO) ---": "---------------------------------------------------",
    "release": "bump package.json package-lock.json CHANGELOG.md --tag && npm run build",
    "version": "auto-changelog --commit-limit false --package && git add CHANGELOG.md",
    "upload": "node scripts/upload.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "fslightbox-react": "^1.4.9",
    "gatsby": "^2.19.11",
    "gatsby-image": "^2.2.40",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-mdx": "^1.0.70",
    "gatsby-plugin-offline": "^3.0.34",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-theme-material-ui": "^1.0.8",
    "gatsby-transformer-sharp": "^2.3.14",
    "gatsby-transformer-yaml": "^2.2.24",
    "github-webcomponents": "^1.3.0",
    "lodash": "^4.17.15",
    "material-ui-audio-player": "^1.0.7",
    "prop-types": "^15.7.2",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-syntax-highlighter": "^12.2.1"
  },
  "devDependencies": {
    "npm-check": "^5.9.0",
    "prettier": "^1.19.1"
  }
}
