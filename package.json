{
  "name": "homepage",
  "version": "6.0.0",
  "private": true,
  "homepage": "https://github.com/rejas/",
  "author": {
    "name": "rejas",
    "url": "https://github.com/rejas/"
  },
  "pre-push": [
    "test"
  ],
  "scripts": {
    "--- MAIN SCRIPTS ---": "------------------------------------------------------------",
    "start": "npm run dev",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "--- AASTRO SCRIPTS ---": "----------------------------------------------------------",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "--- NPM SCRIPTS ---": "-------------------------------------------------------------",
    "format": "prettier --write --ignore-unknown *.{js,json} \"content/**/*.mdx\" \"src/**/*.{js,jsx,css}\"",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "update": "npm-check -u"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.0.7",
    "@astrojs/react": "^4.1.6",
    "@bigheads/core": "^0.3.3",
    "@loadable/component": "^5.16.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "astro": "^5.1.9",
    "fslightbox-react": "^1.7.6",
    "github-webcomponents": "^1.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "npm-check": "^6.0.1",
    "prettier": "^3.4.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
