    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "--- NPM SCRIPTS ---": "-------------------------------------------------------------",
    "format": "prettier --write --ignore-unknown *.{js,json} content/**/*.mdx src/**/*.{js,jsx,css}",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "testLive": "npm run build && npx http-server public",
    "update": "npm-check -u"
  },
